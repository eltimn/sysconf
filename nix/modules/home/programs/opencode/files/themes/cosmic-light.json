{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg": "#F5F7FA",
    "bgPanel": "#FFFFFF",
    "bgElement": "#E8EAED",
    "fg": "#1B1B1B",
    "fgMuted": "#6C6F85",
    "border": "#E2E2E2",
    "borderActive": "#003A99",
    "borderSubtle": "#D0D0D0",
    "accent": "#003A99",
    "accentMuted": "#1E66F5",
    "red": "#D20F39",
    "green": "#40A02B",
    "yellow": "#DF8E1D",
    "blue": "#1E66F5",
    "magenta": "#EA76CB",
    "cyan": "#179299",
    "gray": "#ACB0BE"
  },
  "theme": {
    "primary": { "dark": "accent", "light": "accent" },
    "secondary": { "dark": "accentMuted", "light": "accentMuted" },
    "accent": { "dark": "cyan", "light": "cyan" },
    "error": { "dark": "red", "light": "red" },
    "warning": { "dark": "yellow", "light": "yellow" },
    "success": { "dark": "green", "light": "green" },
    "info": { "dark": "blue", "light": "blue" },
    "text": { "dark": "fg", "light": "fg" },
    "textMuted": { "dark": "fgMuted", "light": "fgMuted" },
    "background": { "dark": "bg", "light": "bg" },
    "backgroundPanel": { "dark": "bgPanel", "light": "bgPanel" },
    "backgroundElement": { "dark": "bgElement", "light": "bgElement" },
    "border": { "dark": "border", "light": "border" },
    "borderActive": { "dark": "borderActive", "light": "borderActive" },
    "borderSubtle": { "dark": "borderSubtle", "light": "borderSubtle" },
    "diffAdded": { "dark": "green", "light": "green" },
    "diffRemoved": { "dark": "red", "light": "red" },
    "diffContext": { "dark": "fgMuted", "light": "fgMuted" },
    "diffHunkHeader": { "dark": "blue", "light": "blue" },
    "diffHighlightAdded": { "dark": "green", "light": "green" },
    "diffHighlightRemoved": { "dark": "red", "light": "red" },
    "diffAddedBg": { "dark": "bgPanel", "light": "bgPanel" },
    "diffRemovedBg": { "dark": "bgPanel", "light": "bgPanel" },
    "diffContextBg": { "dark": "bgPanel", "light": "bgPanel" },
    "diffLineNumber": { "dark": "fgMuted", "light": "fgMuted" },
    "diffAddedLineNumberBg": { "dark": "bgPanel", "light": "bgPanel" },
    "diffRemovedLineNumberBg": { "dark": "bgPanel", "light": "bgPanel" },
    "markdownText": { "dark": "fg", "light": "fg" },
    "markdownHeading": { "dark": "accent", "light": "accent" },
    "markdownLink": { "dark": "blue", "light": "blue" },
    "markdownLinkText": { "dark": "cyan", "light": "cyan" },
    "markdownCode": { "dark": "green", "light": "green" },
    "markdownBlockQuote": { "dark": "fgMuted", "light": "fgMuted" },
    "markdownEmph": { "dark": "magenta", "light": "magenta" },
    "markdownStrong": { "dark": "red", "light": "red" },
    "markdownHorizontalRule": { "dark": "border", "light": "border" },
    "markdownListItem": { "dark": "blue", "light": "blue" },
    "markdownListEnumeration": { "dark": "yellow", "light": "yellow" },
    "markdownImage": { "dark": "blue", "light": "blue" },
    "markdownImageText": { "dark": "cyan", "light": "cyan" },
    "markdownCodeBlock": { "dark": "fg", "light": "fg" },
    "syntaxComment": { "dark": "fgMuted", "light": "fgMuted" },
    "syntaxKeyword": { "dark": "magenta", "light": "magenta" },
    "syntaxFunction": { "dark": "blue", "light": "blue" },
    "syntaxVariable": { "dark": "red", "light": "red" },
    "syntaxString": { "dark": "green", "light": "green" },
    "syntaxNumber": { "dark": "yellow", "light": "yellow" },
    "syntaxType": { "dark": "cyan", "light": "cyan" },
    "syntaxOperator": { "dark": "fg", "light": "fg" },
    "syntaxPunctuation": { "dark": "fgMuted", "light": "fgMuted" }
  }
}
